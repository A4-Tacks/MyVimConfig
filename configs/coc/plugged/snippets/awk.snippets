# begin
snippet begin "BEGIN CodeBlock" b
BEGIN {
	${0}
}
endsnippet

# end
snippet end "END CodeBlock" b
END {
	${0}
}
endsnippet

# block
snippet block "block" b
$1{
	$0
}
endsnippet

snippet for "for" b
for (${1:i} = $2; $1 < $3; ++$1) {
	$4
}
endsnippet

snippet forin "for in" b
for (${1:i} in $2) {
	$3
}
endsnippet
snippet if "if (x)" b
if (${1:x}) {
	$0
}
endsnippet
snippet function "function" b
function ${1:name}(${2:args}, _, ${3:locals}) {
	$0
}
endsnippet
